{
  "name": "team7_watchstrap",
  "description": "test",
  "version": "1.0.0",
  "license": "ISC",
  "main": "index.js",
  "stackblitz": {
    "startCommand": "npm start"
  },
  "scripts": {
    "start-express": "babel-node index.js",
    "test-express": "jest",
    "build": "npm run css",
    "css-compile": "sass --style compressed --source-map --embed-sources --no-error-css --load-path=node_modules static-files/scss/:static-files/css/",
    "css-lint": "stylelint static-files/scss/",
    "css-prefix": "postcss --replace static-files/css/custom.css --use autoprefixer --map",
    "css": "npm-run-all css-compile css-prefix",
    "server": "sirv --dev --no-clear --port 3000",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e html,scss -x \"npm run css\"",
    "test": "npm-run-all css-lint css"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "@popperjs/core": "^2.11.7",
    "bootstrap": "^5.2.3"
  },
  "devDependencies": {
    "mongoose": "^7.0.4",
    "autoprefixer": "^10.4.14",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "sass": "^1.61.0",
    "sirv-cli": "^2.0.2",
    "stylelint": "^15.4.0",
    "stylelint-config-twbs-bootstrap": "^9.0.1",
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.4",
    "dotenv": "^16.0.0"
  },
  "author": "yun seo choi"
}